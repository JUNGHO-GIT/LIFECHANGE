{
  "files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/CVS": true,
		"**/bower_components": true,
		"**/coverage": true,
		"**/tmp": true,
		"**/.mvn": true,
		"**/mvnw": true,
		"**/mvnw.cmd": true,
		"**/*history*": true,
		"**/*bookmark*": true,
		"**/node_modules": true,
		"**/dist": true,
		"**/build": true,
		"**/target": true,
		"**/*.conf": true,
		"**/*.txt": true,
		"**/*Auth*": true,
		"**/*.env*": true
	},
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/build": true,
    "**/target": true,
    "**/*.conf": true,
    "**/*.txt": true,
    "**/*gcloud*": true,
    "**/*Auth*": true,
    "**/*.env*": true,
  },
	"editor.tabSize": 2,
	"explorerExclude.backup": {},
	"editor.codeLens": true,
	"terminal.integrated.fontSize": 12,
	"Html-Js-Css-Analyzer.additionalExtensions":[
		"html",
		"htm",
		"js",
		"jsx",
		"css",
		"scss"
	],
}