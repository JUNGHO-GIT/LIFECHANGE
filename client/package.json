{
  "name": "LIFECHANGE-client",
  "homepage": "/LIFECHANGE",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "index.tsx",
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "off",
      "react-hooks/rules-of-hooks": "warn",
      "@typescript-eslint/no-unused-vars": [
				"off",
				{
					"varsIgnorePattern": "^_",
					"argsIgnorePattern": "^_"
				}
			],
			"@typescript-eslint/no-unused-expressions": "off"
		}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "gcloud-deploy": "node .node/gcloud.cjs --full-deploy",
    "export-autofix": "node .node/autofix.cjs --project tsconfig.json --apply --backup --report .autofix-report.json",
    "pnpm-reset": "pwsh -NoProfile -Command \"try { Stop-Process -Name node -Force -ErrorAction SilentlyContinue } catch {}; Start-Sleep -Milliseconds 200; if (Test-Path 'node_modules') { cmd /c 'rd /s /q \"node_modules\"' } ; if (Test-Path 'package-lock.json') { cmd /c 'del /f /q \"package-lock.json\"' } ; pnpm install\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.2",
    "@mui/system": "^7.3.2",
    "@mui/x-date-pickers": "^8.12.0",
    "@react-hooks-library/core": "^0.6.2",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.12.2",
    "countries-and-timezones": "^3.8.0",
    "date-fns": "^4.1.0",
    "iso-country-currency": "^0.7.2",
    "material-ui-popup-state": "^5.3.6",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mui-file-input": "^7.0.0",
    "react": "^19.1.1",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.3",
    "react-scripts": "^5.0.1",
    "recharts": "^3.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@craco/craco": "^7.1.0",
    "@mui/types": "^7.4.6",
    "@swc/core": "^1.13.20",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/react-router": "^5.1.20",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "react-app-alias": "^2.2.2",
    "swc-minify-webpack-plugin": "^2.1.3",
    "ts-morph": "^27.0.0",
    "ts-prune": "^0.10.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3"
  }
}
